{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [
            "lib"
        ]
    },
    "plugins": ["plugins/markdown", "node_modules/jsdoc-prevent-comment-parse-plugin"],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./docs",
        "recurse": true,
        "tutorials": "./docs/md",
        "readme": "./README.md"
    }
}